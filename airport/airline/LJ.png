			function formCheck(){
				var valid = true;
				$("input").each(function(i, e){
					if($(e).data("valid") != undefined){
						if($(e).data("valid") == "notnull" && $(e).val() == ""){
							alert($(e).data("name")+"를(을) 입력해주세요");
							$(e).focus();
							valid = false;
							return false;
						}
						if($(e).attr("maxlength") < $(e).val().length){
							alert($(e).data("name")+"은(는) 최대"+ $(e).attr("maxlength") +"자까지 입력할 수 있습니다");
							$(e).focus();
							valid = false;
							return false;
						}
						if($(e).attr("min") > $(e).val().length){
							alert($(e).data("name")+"은(는) 최소"+ $(e).attr("min") +"자 이상 입력해야 됩니다.");
							$(e).focus();
							valid = false;
							return false;
						}
					}
				})
				if(!valid) return valid;
				$("textarea").each(function(i, e){
					if($(e).data("valid") != undefined){
						if($(e).data("valid") == "notnull" && $(e).val() == ""){
							alert($(e).data("name")+"를(을) 입력해주세요");
							$(e).focus();
							valid = false;
							return false;
						}
					}
					if($(e).attr("maxlength") < $(e).val().length){
						alert($(e).data("name")+"은(는) 최대"+ $(e).attr("maxlength") +"자까지 입력할 수 있습니다");
						$(e).focus();
						valid = false;
						return false;
					}
				})
				return valid;
			}

